<h1>Students list:</h1>
<br />
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
    </tr>
    </thead>
    <tbody>
        #foreach( $student in $studentList )
        <tr>
            <td>${student.fullName} ${student.middleName} ${student.lastName}</td>
            <td>${student.email}</td>
        </tr>
        #end
    </tbody>
</table>
